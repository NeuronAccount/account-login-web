{
    "extends": ["tslint:latest"],
    "rules": {
        "no-console": [
            false,
            "log",
            "error",
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "object-literal-sort-keys":false,
        "interface-name": [true, "never-prefix"],
        "no-reference": false
    }
}
